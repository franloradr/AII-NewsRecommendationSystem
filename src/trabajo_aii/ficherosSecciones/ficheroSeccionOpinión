					// Banner publi intext2 mobile
					$('article:eq(2)').after($('<aside class="envoltorio-intext2"><div id="intext2" style="margin: 0 auto;"></div></aside>'));
					// Banner publi right2 mobile
					$('article:eq(2)').after($('#adAfterP2'));
