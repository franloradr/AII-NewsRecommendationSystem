		function showPubli() {
			var windowWidth = $(window).width();

			if (windowWidth < 768) {
				// Banner publi intext1 mobile
				$('article').first().after($('<aside class="envoltorio-intext1"><div id="intext1" style="margin: 0 auto;"></div></aside>'));
