<script>
            function detectDevice() {
                windowWidth = window.innerWidth;
                var device = 'desktop';

                if(windowWidth < 768){ //mobile
                    device = 'mobile';
                }else if(windowWidth >= 768 && windowWidth <= 1024) {
                    device = 'tablet';
                }
                return device;
            }
